{
  "name": "two-firewatch",
  "displayName": "two-firewatch",
  "publisher": "Hehk",
  "description": "duotone theme with firewatch colorscheme",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hehk/two-firewatch"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Two Firewatch",
        "uiTheme": "vs-dark",
        "path": "./themes/two-firewatch-color-theme-dark.json"
      },
      {
        "label": "Two Firewatch Light",
        "uiTheme": "vs",
        "path": "./themes/two-firewatch-color-theme-light.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node ./src/build.ts",
    "dev": "ts-node ./src/dev.ts",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.17.0"
  }
}
